{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "Elasticsearch": {
    "Url": "http://localhost:9200",
    "DefaultIndex": "gonomads",
    "EnableDebugMode": true
  },
  "Consul": {
    "Address": "http://localhost:7500"
  },
  "RabbitMQ": {
    "Host": "localhost"
  },
  "IndexMaintenance": {
    "Enabled": false,
    "InitialDelayMinutes": 1,
    "VerifyIntervalMinutes": 30
  },
  "StartupSync": {
    "Enabled": true,
    "ForceSync": false,
    "MaxRetries": 10,
    "RetryIntervalSeconds": 3,
    "WaitForServicesSeconds": 5,
    "MaxSyncRetries": 2,
    "MinDocumentThreshold": 1
  }
}
