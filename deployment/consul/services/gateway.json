{
  "service": {
    "name": "gateway",
    "id": "gateway-1",
    "tags": ["api", "gateway", "dotnet"],
    "address": "gateway",
    "port": 8080,
    "meta": {
      "version": "1.0.0",
      "protocol": "http"
    },
    "checks": [
      {
        "id": "gateway-health",
        "name": "Gateway Health Check",
        "http": "http://gateway:8080/health",
        "interval": "10s",
        "timeout": "5s"
      }
    ]
  }
}
